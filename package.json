{
  "name": "@uni-web-lab/vue3-spa-template",
  "version": "0.0.1",
  "description": "vue3-spa-template是一个vue3 + vite4搭建的支持h5单页spa场景的标准项目模板，并对日常的开发工具都做了封装。",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite serve --mode development",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue src --fix",
    "format": "prettier --write src/",
    "tag": "bash build/new_tag.sh",
    "prepare": "husky"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoroer/uni-web-templates.git"
  },
  "keywords": [
    "cli",
    "template",
    "uni-web"
  ],
  "author": "zoroer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zoroer/uni-web-templates/issues"
  },
  "homepage": "https://github.com/zoroer/uni-web-templates#readme",
  "dependencies": {
    "axios": "^1.6.8",
    "pinia": "^2.1.7",
    "unplugin-vue-components": "^0.26.0",
    "vant": "^4.8.10",
    "vconsole": "^3.15.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vconsole": "^2.1.1",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@vant/auto-import-resolver": "^1.1.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.0.3",
    "vite": "^5.1.6"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
